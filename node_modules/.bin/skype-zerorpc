#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//usr/bin/python" ]; then
  "$basedir//usr/bin/python"  "$basedir/../skype-api/bin/skype-zerorpc" "$@"
  ret=$?
else 
  /usr/bin/python  "$basedir/../skype-api/bin/skype-zerorpc" "$@"
  ret=$?
fi
exit $ret
